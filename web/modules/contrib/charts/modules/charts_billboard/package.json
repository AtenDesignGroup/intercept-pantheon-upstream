{
  "name": "drupal-charts-billboard-libraries",
  "private": true,
  "version": "1.0.0",
  "description": "Manages Billboard.js JS libraries for the Drupal Charts module.",
  "scripts": {
    "libraries:copy": "rimraf ../../../../../libraries/billboard ../../../../../libraries/d3 && copy-files-from-to"
  },
  "dependencies": {
    "billboard.js": "^3.16.0",
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.9.0",
    "rimraf": "^5.0.7"
  },
  "copyFiles": [
    {
      "from": "../../../../../../node_modules/billboard.js/dist/billboard.min.css",
      "to": "../../../../../libraries/billboard/dist/billboard.min.css"
    },
    {
      "from": "../../../../../../node_modules/billboard.js/dist/billboard.min.js",
      "to": "../../../../../libraries/billboard/dist/billboard.min.js"
    },
    {
      "from": "../../../../../../node_modules/d3/dist/d3.min.js",
      "to": "../../../../../libraries/d3/d3.min.js"
    }
  ]
}
