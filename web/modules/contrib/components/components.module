<?php

/**
 * @file
 * Hooks for the Components module.
 */

use Drupal\Core\Hook\Attribute\LegacyHook;
use Drupal\components\Hook\ComponentsRegistryCacheInvalidator;

/**
 * Implements hook_theme().
 */
#[LegacyHook] /* @phpstan-ignore attribute.notFound */
function components_theme(): array {
  $invalidator = new ComponentsRegistryCacheInvalidator();
  return $invalidator->invalidate();
}
