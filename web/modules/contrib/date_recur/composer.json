{
  "name": "drupal/date_recur",
  "type": "drupal-module",
  "description": "Recurring Dates Field",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/date_recur",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/date_recur",
    "source": "http://cgit.drupalcode.org/date_recur"
  },
    "repositories": {
        "drupalorg": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "canonical": false
        }
    },
  "require": {
    "php": ">=8.1",
    "drupal/core": "^10",
    "rlanvin/php-rrule": "^2"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1",
    "drupal/coder": "^8.3",
    "drupal/token": "^1.5",
    "mglaman/phpstan-drupal": "^1",
    "phpstan/phpstan": "^1",
    "phpstan/phpstan-deprecation-rules": "*",
    "previousnext/coding-standard": "^0.1.0"
  },
  "suggest": {
    "drupal/date_recur_interactive": "Provides an interactive recurring date widget."
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
