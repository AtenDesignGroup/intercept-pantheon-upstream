{
  "name": "drupal/intercept",
  "type": "drupal-module",
  "description": "Drupal Intercept module.",
  "keywords": [
    "Drupal"
  ],
  "license": "MIT",
  "homepage": "https://www.drupal.org/project/intercept",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/intercept",
    "source": "http://cgit.drupalcode.org/intercept"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "git",
      "url": "https://github.com/lordelph/php-sip2.git"
    }
  ],
  "require": {
    "drupal/addtocalendar": "^3.1",
    "drupal/address": "^2.0",
    "drupal/charts": "^5.0",
    "drupal/clientside_validation": "^4.0",
    "drupal/consumer_image_styles": "^4.0.8",
    "drupal/csv_serialization": "^4.0",
    "drupal/date_recur": "^3.9",
    "drupal/date_recur_modular": "^3.1",
    "drupal/duration_field": "^2.1",
    "drupal/entity_browser": "^2.0",
    "drupal/entity": "^1.0",
    "drupal/existing_values_autocomplete_widget": "^2.0@alpha",
    "drupal/externalauth": "^2.0",
    "drupal/field_group": "^3.0",
    "drupal/focal_point": "^2.0",
    "drupal/flag": "^4.0",
    "drupal/fullcalendar_block": "^1.0",
